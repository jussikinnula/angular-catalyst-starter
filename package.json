{
  "name": "angular-catalyst-starter",
  "version": "0.0.4",
  "description": "A simple starting point to build a web application using Catalyst Framework and Angular frontend.",
  "homepage": "https://github.com/jussikinnula/angular-catalyst-starter",
  "repository": "https://github.com/jussikinnula/angular-catalyst-starter",
  "author": {
    "name": "Jussi Kinnula",
    "email": "jussi.kinnula@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack --progress",
    "clean": "rimraf ./root",
    "dev:plackup": "cross-env PLACK_ENV=development plackup --port 12051",
    "dev:webpack": "cross-env NODE_ENV=development webpack-dev-server",
    "dev": "concurrently \"npm run dev:plackup\" \"npm run dev:webpack\"",
    "start": "plackup",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@types/hammerjs": "^2.0.34",
    "@types/mime": "^1.3.1",
    "@types/node": "^8.0.14",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "exports-loader": "^0.6.4",
    "express": "^4.15.3",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "moment": "^2.18.1",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.3.1",
    "ts-node": "~3.2.1",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "webpack-dev-server": "~2.5.1"
  }
}
